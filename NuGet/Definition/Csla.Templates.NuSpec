  <files>
    <!-- Snippets for Tools folder -->
    <file src="tools\init.ps1" target="tools" />
    <file src="tools\uninstall.ps1" target="tools" />
    <file src="..\..\Support\Snippets\cs\*.snippet" target="tools\Snippets\cs" />
    <file src="..\..\Support\Snippets\vb\*.snippet" target="tools\Snippets\vb" />
    <file src="..\..\Support\TemplateInstaller\CslaTemplates\ItemTemplates\CSharp\Csla4\*.zip" target="tools\Templates\cs" />
    <file src="..\..\Support\TemplateInstaller\CslaTemplates\ItemTemplates\VisualBasic\Csla4\*.zip" target="tools\Templates\vb" />
    <!-- resources -->
    <file src="..\..\Support\Logos\csla.png" target="images\" />
